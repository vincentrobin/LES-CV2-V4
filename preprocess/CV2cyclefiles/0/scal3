FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      scal3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            zeroGradient;
    }

    injector1
    {
        type            zeroGradient;
    }

    injector2
    {
        type            zeroGradient;
    }

    injector3
    {
        type            zeroGradient;
    }

    injector4
    {
        type            zeroGradient;
    }
}

