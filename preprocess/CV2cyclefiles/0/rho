FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1;
    }

    outlet
    {
        type            calculated;
        value           uniform 1;
    }

    wall
    {
        type            calculated;
        value           uniform 1;
    }

    injector1
    {
        type            calculated;
        value           uniform 1;
    }

    injector2
    {
        type            calculated;
        value           uniform 1;
    }

    injector3
    {
        type            calculated;
        value           uniform 1;
    }

    injector4
    {
        type            calculated;
        value           uniform 1;
    }

}


